{
  "root": true,
  "env": {
    "node": false,
    "browser": true,
    "es2022": true
  },
  "extends": [
    "plugin:prettier/recommended",
    "plugin:vue/vue3-essential",
    "eslint:recommended",
    "@vue/eslint-config-typescript"
  ],
  "rules": {
    "vue/multi-word-component-names": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}